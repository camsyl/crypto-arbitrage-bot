# Ethereum Node Configuration
PRIVATE_KEY=your_ethereum_private_key_here

# RPC Provider Selection - set to "infura", "alchemy", "public", or "google"
RPC_PROVIDER=alchemy

# Infura Keys
INFURA_KEY=your_infura_key_here

# Alchemy Keys
ALCHEMY_KEY=your_alchemy_key_here

# Google RPC
GOOGLE_RPC_KEY=your_google_key_here
GOOGLE_PROJECT_ID=your_google_project_id_here

# Etherscan API Key (for contract verification)
ETHERSCAN_API_KEY=your_etherscan_api_key_here

# Contract Addresses 
FLASH_LOAN_CONTRACT_ADDRESS=your_deployed_contract_address_here

# Exchange API Keys (if using CEX integration)
# COINBASE_API_KEY=your_coinbase_api_key_here
# COINBASE_API_SECRET=your_coinbase_api_secret_here
